@prefix : <http://rukmal.me/precis#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://rukmal.me/precis> .

<http://rukmal.me/precis> rdf:type owl:Ontology ;
                           owl:versionIRI <http://rukmal.me/precis/1.0.0> ;
                           <http://purl.org/dc/elements/1.1/creator> <https://orcid.org/0000-0002-8628-6753> ;
                           rdfs:comment "The Precis ontology is designed to encapsulate portfolio information about an individual. This includes information that would traditionally be presented in a Resume/CV. This ontology is designed to be used with the Precis toolkit; see http://github.com/rukmal/precis for more."^^xsd:string ;
                           owl:versionInfo "1.0.0"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gMonth
xsd:gMonth rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://rukmal.me/precis#awardedDegree
:awardedDegree rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :relational_property ;
               owl:inverseOf :degreeUniversity ;
               rdf:type owl:InverseFunctionalProperty ;
               rdfs:domain :Organization ;
               rdfs:range :Degree ;
               rdfs:comment "The Degree which was awarded at this organization (should be a pointer to a top-level organization)." ;
               rdfs:label "Degree awarded" .


###  http://rukmal.me/precis#degreeDepartment
:degreeDepartment rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :relational_property ;
                  rdfs:domain :Degree ;
                  rdfs:range :Organization ;
                  rdfs:comment "Department awarding the degree (eg: \"Department of Financial Engineering\", or \"Department of Finance and Business Economics\")." ;
                  rdfs:label "Department awarding the Degree" .


###  http://rukmal.me/precis#degreeSchool
:degreeSchool rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :relational_property ;
              rdfs:domain :Degree ;
              rdfs:range :Organization ;
              rdfs:comment "The School that awards the degree. For example, this would be the \"Foster School of Business\", or \"College of Arts and Sciences\"." ;
              rdfs:label "School awarding the Degree" .


###  http://rukmal.me/precis#degreeUniversity
:degreeUniversity rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :relational_property ;
                  rdfs:domain :Degree ;
                  rdfs:range :Organization ;
                  rdfs:comment "The University (concept: Organization) at which the degree was awarded. This corresponds to the top-level instituion; further object properties exist to specify particular schools/departments. That is, this should point to the organization for the Univesity and not the Department. Eg: awardedAt 'University of Washington'; and should NOT be awardedAt 'Foster School of Business'." ;
                  rdfs:label "Organization awarding the Degree" .


###  http://rukmal.me/precis#employedAt
:employedAt rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :relational_property ;
            rdfs:domain :WorkExperience ;
            rdfs:range :Organization ;
            rdfs:comment "Organization where an instance of Work Experience is taking place." ;
            rdfs:label "Employment Organization" .


###  http://rukmal.me/precis#hasChildOrganization
:hasChildOrganization rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :relational_property ;
                      owl:inverseOf :hasParentOrganization ;
                      rdf:type owl:InverseFunctionalProperty ;
                      rdfs:domain :Organization ;
                      rdfs:range :Organization ;
                      rdfs:comment "Child organization pointer, inverse of the 'hasParentOrganization' property." ;
                      rdfs:label "Child Organization" .


###  http://rukmal.me/precis#hasDescription
:hasDescription rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :relational_property ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Organization
                                            :SkillGroup
                                            :WorkExperience
                                          )
                            ] ;
                rdfs:range :Description .


###  http://rukmal.me/precis#hasParentOrganization
:hasParentOrganization rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :relational_property ;
                       rdfs:domain :Organization ;
                       rdfs:range :Organization ;
                       rdfs:comment "Pointer to a \"parent\" organization instance. For example, the \"Foster School of Business\" hasParentOrganization \"University of Washington\"." ;
                       rdfs:label "Parent Organization" .


###  http://rukmal.me/precis#relational_property
:relational_property rdf:type owl:ObjectProperty .


###  http://rukmal.me/precis#taughtAt
:taughtAt rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :relational_property ;
          rdfs:domain :Course ;
          rdfs:range :Organization ;
          rdfs:comment "The organization at which the course was taught. This is an optional field, but ideally if this is included, the department and course codes should be too." ;
          rdfs:label "Organization which taught the course" .


#################################################################
#    Data properties
#################################################################

###  http://rukmal.me/precis#commonProperties
:commonProperties rdf:type owl:DatatypeProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Course
                                              :Organization
                                              :Publication
                                              :Skill
                                              :SkillGroup
                                              :WorkExperience
                                            )
                              ] ;
                  rdfs:comment "Common properties to many concepts." .


###  http://rukmal.me/precis#courseProperties
:courseProperties rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Course ;
                  rdfs:comment "Unique properties for the Course concept." .


###  http://rukmal.me/precis#degreeConcentration
:degreeConcentration rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :degreeProperties ;
                     rdfs:range xsd:anyURI ;
                     rdfs:comment "Concentration of the Degree. For example, \"Finance and Business Economics\", or \"Computational Finance\". This should point to a DBpedia entity corresponding to the Degree being awarded." ;
                     rdfs:label "Degree Concentration" .


###  http://rukmal.me/precis#degreeGPA
:degreeGPA rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :degreeProperties ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:double ;
                        owl:withRestrictions ( [ xsd:minInclusive "0.0"^^xsd:double
                                               ]
                                               [ xsd:maxInclusive "4.0"^^xsd:double
                                               ]
                                             )
                      ] ;
           rdfs:comment "Grade (GPA; 4.0 scale) for the Degree (optional for obvious reasons :))." ;
           rdfs:label "Degree GPA (on a 4.0 scale)" .


###  http://rukmal.me/precis#degreeProperties
:degreeProperties rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Degree ;
                  rdfs:comment "Unique properties for the Degree concept." .


###  http://rukmal.me/precis#degreeType
:degreeType rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :degreeProperties ;
            rdfs:range xsd:anyURI ;
            rdfs:comment "Type of the Degree. For example, \"Bachelor of Arts\", or \"Master of Science\". This should point to a DBpedia entity corresponding to the Degree being awarded." ;
            rdfs:label "Degree Type" .


###  http://rukmal.me/precis#descriptionProperties
:descriptionProperties rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Description ;
                       rdfs:comment "Unique properties for the Description concept." .


###  http://rukmal.me/precis#endMonth
:endMonth rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :workExperienceProperties ;
          rdfs:range xsd:gMonth ;
          rdfs:comment "End date (month) of the work experience instance." ;
          rdfs:label "End Date (month)" .


###  http://rukmal.me/precis#endYear
:endYear rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :workExperienceProperties ;
         rdfs:range xsd:gYear ;
         rdfs:comment "End date (year) of the work experience instance." ;
         rdfs:label "End Date (year)" .


###  http://rukmal.me/precis#externalResource
:externalResource rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :commonProperties ;
                  rdfs:range xsd:anyURI ;
                  rdfs:comment "Reference to an external resource that describes the same entity. This would typically be a reference to an existing DBpedia concept that describes the same entity. DBpedia references are not directly used here to mitigate the fact that certain user-created entitites will not have existing concepts on DBpedia, and so that we can ensure the existence of some other descriptive fields." ;
                  rdfs:label "External resource" .


###  http://rukmal.me/precis#hasAuthors
:hasAuthors rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :publicationProperties ;
            rdfs:range xsd:string ;
            rdfs:comment "List of authors (Human readable for now). This list should mirror the order of authors in the publication. Note that eventually this will be replaced by pointers to individual \"Author\" Classes." ;
            rdfs:label "Author List" .


###  http://rukmal.me/precis#hasCourseCode
:hasCourseCode rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :courseProperties ;
               rdfs:range xsd:string ;
               rdfs:comment "Code of the course (this would imply that an organization is referenced too). For example: 420, 690, 069, etc." ;
               rdfs:label "Course code" .


###  http://rukmal.me/precis#hasDOI
:hasDOI rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf :publicationProperties ;
        rdfs:range xsd:anyURI ;
        rdfs:comment "Digital Object Identifier (DOI) of the publication. Including this will allow for later expansion if an inference agent is ever added to get more metadata using this." ;
        rdfs:label "Digital Object Identifier (DOI) of the Publication" .


###  http://rukmal.me/precis#hasDepartmentCode
:hasDepartmentCode rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :courseProperties ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Code of the department (this would imply that an organization is referenced too). For example: FIN, CFRM, FE, etc." ;
                   rdfs:label "Department code" .


###  http://rukmal.me/precis#hasDescription
:hasDescription rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :publicationProperties ;
                rdfs:range xsd:string .


###  http://rukmal.me/precis#hasImage
:hasImage rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :workExperienceProperties ;
          rdfs:range xsd:anyURI ;
          rdfs:comment "URL to image related to work experience. Can be used when displaying the information at consumption." ;
          rdfs:label "Image" .


###  http://rukmal.me/precis#hasMedia
:hasMedia rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :workExperienceProperties ;
          rdfs:range xsd:anyURI ;
          rdfs:comment "Additional media can be included here. For example, it could link to product/team pages, news articles, etc." ;
          rdfs:label "Media" .


###  http://rukmal.me/precis#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :commonProperties ;
         rdfs:range xsd:string ;
         rdfs:comment "Human readable name for the entity; for display." ;
         rdfs:label "Human readable name" .


###  http://rukmal.me/precis#hasPriority
:hasPriority rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :descriptionProperties ;
             rdfs:range xsd:nonNegativeInteger ;
             rdfs:comment "Priority level of the description (used for deciding truncation in toolkit). This is automatically set to the position of the string in the input \"description\" array of descriptions, when data is loaded using the command line tool." ;
             rdfs:label "description priority level" .


###  http://rukmal.me/precis#hasStatus
:hasStatus rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :publicationProperties ;
           rdfs:range xsd:string ;
           rdfs:comment "Status of the publication. That is, this property should reflect a paper being \"In Progress\", or \"Under Review\"." ;
           rdfs:label "Publication Status" .


###  http://rukmal.me/precis#hasText
:hasText rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :descriptionProperties ;
         rdfs:range xsd:string ;
         rdfs:comment "Text of the description. This should be treated as a single \"bullet point\" on a traditional Resume/CV." ;
         rdfs:label "description text" .


###  http://rukmal.me/precis#hasWebsite
:hasWebsite rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :commonProperties ;
            rdfs:range xsd:anyURI ;
            rdfs:comment "External website of the entity (not used for ontology extension; treat this as user-facing)." ;
            rdfs:label "Entity website" .


###  http://rukmal.me/precis#inCity
:inCity rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf :commonProperties ;
        rdfs:range xsd:string ;
        rdfs:comment "The city of the entity." ;
        rdfs:label "City" .


###  http://rukmal.me/precis#inConferenceOrJournal
:inConferenceOrJournal rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :publicationProperties ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Conference/Journal name. This will eventually be replaced with a more semantically-correct representation of this information." ;
                       rdfs:label "Conference/Journal Name" .


###  http://rukmal.me/precis#inCountry
:inCountry rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :commonProperties ;
           rdfs:range xsd:string ;
           rdfs:comment "Country of the entity." ;
           rdfs:label "Country" .


###  http://rukmal.me/precis#inPublication
:inPublication rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :publicationProperties ;
               rdfs:range xsd:string ;
               rdfs:comment "Publication name containing the specific publication. As with the other fields in the 'Publication' concept, this will probably be replaced eventually." ;
               rdfs:label "Publication Name" .


###  http://rukmal.me/precis#inState
:inState rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :commonProperties ;
         rdfs:range xsd:string ;
         rdfs:comment "State of the entity (United States only)." ;
         rdfs:label "State" .


###  http://rukmal.me/precis#inYear
:inYear rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf :publicationProperties ;
        rdfs:range xsd:gYear ;
        rdfs:comment "Year the publication was published." ;
        rdfs:label "Publication Year" .


###  http://rukmal.me/precis#jobTitle
:jobTitle rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :workExperienceProperties ;
          rdfs:range xsd:string ;
          rdfs:comment "Job Title for the work experience instance. Each work experience instance can have multiple job titles. For example, both \"Software Engineering Intern\" and \"Software Engineer\" could be job titles of the same work experience instance." ;
          rdfs:label "Job Title" .


###  http://rukmal.me/precis#monthAwarded
:monthAwarded rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :degreeProperties ;
              rdfs:range xsd:gMonth ;
              rdfs:comment "The month that the degree was earned." ;
              rdfs:label "month awarded" .


###  http://rukmal.me/precis#publicationProperties
:publicationProperties rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Publication ;
                       rdfs:comment "Note that all properties in this class, and (probably) the whole 'Publication' concept will be replaced with a more semantically-accurate representation of this data. This is temporary cuz I'm lazy." ,
                                    "Unique properties for the Publication concept." .


###  http://rukmal.me/precis#startMonth
:startMonth rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :workExperienceProperties ;
            rdfs:range xsd:gMonth ;
            rdfs:comment "Start date (month) of the work experience instance." ;
            rdfs:label "Start Date (month)" .


###  http://rukmal.me/precis#startYear
:startYear rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :workExperienceProperties ;
           rdfs:range xsd:gYear ;
           rdfs:comment "Start date (year) of the work experience instance." ;
           rdfs:label "Start Date (year)" .


###  http://rukmal.me/precis#withGrade
:withGrade rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :courseProperties ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:double ;
                        owl:withRestrictions ( [ xsd:minInclusive "0.0"^^xsd:double
                                               ]
                                               [ xsd:maxInclusive "4.0"^^xsd:double
                                               ]
                                             )
                      ] ;
           rdfs:comment "Grade (GPA; 4.0 scale) received in the course (optional for obvious reasons :))." ;
           rdfs:label "Course GPA (on a 4.0 scale)" .


###  http://rukmal.me/precis#workExperienceProperties
:workExperienceProperties rdf:type owl:DatatypeProperty ;
                          rdfs:domain :WorkExperience ;
                          rdfs:comment "Unique properties for the Work Experience concept." .


###  http://rukmal.me/precis#yearAwarded
:yearAwarded rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :degreeProperties ;
             rdfs:range xsd:gYear ;
             rdfs:comment "The year that the degree was awarded." ;
             rdfs:label "year awarded" .


#################################################################
#    Classes
#################################################################

###  http://rukmal.me/precis#Accolade
:Accolade rdf:type owl:Class .


###  http://rukmal.me/precis#Activity
:Activity rdf:type owl:Class ;
          rdfs:subClassOf :ActivityType .


###  http://rukmal.me/precis#ActivityType
:ActivityType rdf:type owl:Class .


###  http://rukmal.me/precis#Award
:Award rdf:type owl:Class ;
       rdfs:subClassOf :Accolade .


###  http://rukmal.me/precis#Certification
:Certification rdf:type owl:Class ;
               rdfs:subClassOf :Accolade .


###  http://rukmal.me/precis#Course
:Course rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :taughtAt ;
                          owl:someValuesFrom :Organization
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasCourseCode ;
                          owl:someValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasDepartmentCode ;
                          owl:someValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :withGrade ;
                          owl:someValuesFrom xsd:double
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasName ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasWebsite ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:anyURI
                        ] ;
        rdfs:comment "A course. This would typically be an item that is included in the \"Relevant Coursework\" section of a Resume/CV." ;
        rdfs:label "Course" .


###  http://rukmal.me/precis#Degree
:Degree rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :degreeDepartment ;
                          owl:someValuesFrom :Organization
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :degreeSchool ;
                          owl:someValuesFrom :Organization
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :degreeUniversity ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Organization
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :degreeConcentration ;
                          owl:someValuesFrom xsd:anyURI
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :degreeGPA ;
                          owl:someValuesFrom xsd:double
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :degreeType ;
                          owl:someValuesFrom xsd:anyURI
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasName ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :inCity ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :inState ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :monthAwarded ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:gMonth
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :yearAwarded ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:gYear
                        ] ;
        rdfs:comment "A Degree, which was completed at a specific institution, in a specific school and department. Optional media links to pictures can be included for rendering at the time of presentation. Records the year and month that the degree was (or is expected to be) awarded." ;
        rdfs:label "Degree" .


###  http://rukmal.me/precis#Description
:Description rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasPriority ;
                               owl:someValuesFrom xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasText ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ;
             rdfs:comment "This description is associated with any other concept, providing a description. It has the capability to track priority levels, which can be used to intelligently truncate a series of descriptions when creating a Resume/CV with the command line toolkit." ;
             rdfs:label "Description for a given item" .


###  http://rukmal.me/precis#KnowledgeArea
:KnowledgeArea rdf:type owl:Class .


###  http://rukmal.me/precis#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :hasParentOrganization ;
                                owl:someValuesFrom :Organization
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasDescription ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Description
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :externalResource ;
                                owl:someValuesFrom xsd:anyURI
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasName ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasWebsite ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:anyURI
                              ] ;
              rdfs:comment "An organization; this can be a company, university, a specific university department, etc." ;
              rdfs:label "Organization" .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Organization ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty :hasDescription ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Description
                       ] ;
   rdfs:comment "This should be used to add an additional descriptive string if necessary. For example, this could be a team name or a descriptive string (eg: \"Startup Company\", etc.)."
 ] .


###  http://rukmal.me/precis#Portfolio
:Portfolio rdf:type owl:Class .


###  http://rukmal.me/precis#Project
:Project rdf:type owl:Class ;
         rdfs:subClassOf :Portfolio .


###  http://rukmal.me/precis#Publication
:Publication rdf:type owl:Class ;
             rdfs:subClassOf :Portfolio .


###  http://rukmal.me/precis#Skill
:Skill rdf:type owl:Class ;
       rdfs:subClassOf :SkillGroup ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :externalResource ;
                         owl:someValuesFrom xsd:anyURI
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasName ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:string
                       ] ;
       rdfs:comment "An individual skill. Each skill belongs to a specific SkillGroup. For example; \"Python\", \"R\", and \"Java\" would all be skills, in the \"Programming Languages\" SkillGroup." ;
       rdfs:label "Skill" .


###  http://rukmal.me/precis#SkillGroup
:SkillGroup rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasDescription ;
                              owl:someValuesFrom :Description
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :externalResource ;
                              owl:someValuesFrom xsd:anyURI
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasName ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ;
