@prefix : <http://precis.rukmal.me/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://precis.rukmal.me/ontology> .

<http://precis.rukmal.me/ontology> rdf:type owl:Ontology ;
                                    owl:versionIRI <http://precis.rukmal.me/ontology/1.5.1> ;
                                    <http://purl.org/dc/elements/1.1/abstract> """The Precis ontology is designed to encapsulate portfolio information about an individual. This includes information that would traditionally be presented in a Resume/CV. This ontology is designed to be used with the Precis toolkit; see http://precis.rukmal.me for more.

Currently, this ontology powers a templating engine to generate dyanmic resumes, but has the potential to power other personal data driven experiences.""" ;
                                    <http://purl.org/dc/elements/1.1/creator> "Rukmal Weerawarana" ;
                                    <http://purl.org/dc/elements/1.1/title> "Precis Ontology" ;
                                    <http://purl.org/dc/terms/created> "2019-04-07T00:00:00"^^xsd:dateTime ;
                                    <http://purl.org/dc/terms/creator> <http://rukmal.me> ;
                                    <http://purl.org/dc/terms/modified> "2019-04-19T00:00:00"^^xsd:dateTime ;
                                    <http://purl.org/ontology/bibo/doi> <http://purl.org/precis> ;
                                    <http://purl.org/vocab/vann/preferredNamespacePrefix> "precis" ;
                                    <http://purl.org/vocab/vann/preferredNamespaceUri> "http://precis.rukmal.me/ontology" ;
                                    <http://schema.org/license> <https://github.com/rukmal/precis/blob/master/LICENSE> ;
                                    rdfs:comment """The Precis ontology is designed to encapsulate portfolio information about an individual. This includes information that would traditionally be presented in a Resume/CV. This ontology is designed to be used with the Precis toolkit; see http://precis.rukmal.me for more.

Currently, this ontology powers a templating engine to generate dyanmic resumes, but has the potential to power other personal data driven experiences.""" ;
                                    owl:priorVersion <https://github.com/rukmal/precis/releases/download/1.5.0/precis_ontology.rdf> ;
                                    owl:versionInfo "1.5.1" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/abstract
<http://purl.org/dc/elements/1.1/abstract> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
<http://purl.org/dc/terms/modified> rdf:type owl:AnnotationProperty .


###  http://purl.org/ontology/bibo/doi
<http://purl.org/ontology/bibo/doi> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://schema.org/creator
<http://schema.org/creator> rdf:type owl:AnnotationProperty .


###  http://schema.org/description
<http://schema.org/description> rdf:type owl:AnnotationProperty .


###  http://schema.org/license
<http://schema.org/license> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#priorVersion
owl:priorVersion rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gMonth
xsd:gMonth rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://precis.rukmal.me/ontology#affiliatedWith
:affiliatedWith rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasObject ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Accolade
                                            :Portfolio
                                          )
                            ] ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :Degree
                                           :WorkExperience
                                         )
                           ] ;
                rdfs:comment "Affiliation for a Portfolio Instance or Accolade Instance. Maps a given instance to an instance of 'Degree' or 'WorkExperience'." ;
                rdfs:label "Affiliation" .


###  http://precis.rukmal.me/ontology#awardedDegree
:awardedDegree rdf:type owl:ObjectProperty ;
               owl:inverseOf :degreeUniversity ;
               rdfs:domain :Organization ;
               rdfs:range :Degree ;
               rdfs:comment "The Degree which was awarded at this organization (should be a pointer to a top-level organization)." ;
               rdfs:label "Degree awarded" .


###  http://precis.rukmal.me/ontology#degreeDepartment
:degreeDepartment rdf:type owl:ObjectProperty ;
                  rdfs:domain :Degree ;
                  rdfs:range :Organization ;
                  rdfs:comment "Department awarding the degree (eg: \"Department of Financial Engineering\", or \"Department of Finance and Business Economics\")." ;
                  rdfs:label "Department awarding the Degree" .


###  http://precis.rukmal.me/ontology#degreeSchool
:degreeSchool rdf:type owl:ObjectProperty ;
              rdfs:domain :Degree ;
              rdfs:range :Organization ;
              rdfs:comment "The School that awards the degree. For example, this would be the \"Foster School of Business\", or \"College of Arts and Sciences\"." ;
              rdfs:label "School awarding the Degree" .


###  http://precis.rukmal.me/ontology#degreeUniversity
:degreeUniversity rdf:type owl:ObjectProperty ;
                  rdfs:domain :Degree ;
                  rdfs:range :Organization ;
                  rdfs:comment "The University (concept: Organization) at which the degree was awarded. This corresponds to the top-level instituion; further object properties exist to specify particular schools/departments. That is, this should point to the organization for the Univesity and not the Department. Eg: awardedAt 'University of Washington'; and should NOT be awardedAt 'Foster School of Business'." ;
                  rdfs:label "Organization awarding the Degree" .


###  http://precis.rukmal.me/ontology#employedAt
:employedAt rdf:type owl:ObjectProperty ;
            rdfs:domain :WorkExperience ;
            rdfs:range :Organization ;
            rdfs:comment "Organization where an instance of Work Experience is taking place." ;
            rdfs:label "Employment Organization" .


###  http://precis.rukmal.me/ontology#hasActivity
:hasActivity rdf:type owl:ObjectProperty ;
             owl:inverseOf :inActivityType ;
             rdfs:domain :ActivityType ;
             rdfs:range :Activity ;
             rdfs:comment "An 'Activity' that is a part of an 'ActivityType'." ;
             rdfs:label "Constituent Activity" .


###  http://precis.rukmal.me/ontology#hasChildOrganization
:hasChildOrganization rdf:type owl:ObjectProperty ;
                      owl:inverseOf :hasParentOrganization ;
                      rdf:type owl:TransitiveProperty ;
                      rdfs:domain :Organization ;
                      rdfs:range :Organization ;
                      rdfs:comment "Child organization pointer, inverse of the 'hasParentOrganization' property." ;
                      rdfs:label "Child Organization" .


###  http://precis.rukmal.me/ontology#hasDescription
:hasDescription rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Accolade
                                            :ActivityType
                                            :Course
                                            :KnowledgeArea
                                            :Organization
                                            :Portfolio
                                            :SkillGroup
                                            :WorkExperience
                                          )
                            ] ;
                rdfs:range :Description ;
                rdfs:comment "Instance of 'Description', which describes the Thing pointing to it." ;
                rdfs:label "Description" .


###  http://precis.rukmal.me/ontology#hasObject
:hasObject rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Degree
                                       :WorkExperience
                                     )
                       ] ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Accolade
                                      :Portfolio
                                    )
                      ] ;
           rdfs:comment "Object that was affiliated with the Thing pointing to it. Here, \"Object\" would be typically a 'Portfolio' or 'Accolade' instance, and \"Thing\" would be a 'Degree' or 'WorkExperience' instance." ;
           rdfs:label "Constituent Object" .


###  http://precis.rukmal.me/ontology#hasParentOrganization
:hasParentOrganization rdf:type owl:ObjectProperty ,
                                owl:TransitiveProperty ;
                       rdfs:domain :Organization ;
                       rdfs:range :Organization ;
                       rdfs:comment "Pointer to a \"parent\" organization instance. For example, the \"Foster School of Business\" hasParentOrganization \"University of Washington\"." ;
                       rdfs:label "Parent Organization" .


###  http://precis.rukmal.me/ontology#hasSkill
:hasSkill rdf:type owl:ObjectProperty ;
          owl:inverseOf :inSkillGroup ;
          rdfs:domain :SkillGroup ;
          rdfs:range :Skill ;
          rdfs:comment "A skill that is a part of a 'SkillGroup'." ;
          rdfs:label "Constituent Skill" .


###  http://precis.rukmal.me/ontology#hasSubject
:hasSubject rdf:type owl:ObjectProperty ;
            owl:inverseOf :inKnowledgeArea ;
            rdfs:domain :KnowledgeArea ;
            rdfs:range :Subject ;
            rdfs:comment "A subject that is a part of a 'KnowledgeArea'." ;
            rdfs:label "Constituent Subject" .


###  http://precis.rukmal.me/ontology#inActivityType
:inActivityType rdf:type owl:ObjectProperty ;
                rdfs:domain :Activity ;
                rdfs:range :ActivityType ;
                rdfs:comment "The ActivityType containing the Activity." ;
                rdfs:label "Parent Activity Type" .


###  http://precis.rukmal.me/ontology#inKnowledgeArea
:inKnowledgeArea rdf:type owl:ObjectProperty ;
                 rdfs:domain :Subject ;
                 rdfs:range :KnowledgeArea ;
                 rdfs:comment "The knowledge area containing the subject." ;
                 rdfs:label "Parent Knowledge Area" .


###  http://precis.rukmal.me/ontology#inSkillGroup
:inSkillGroup rdf:type owl:ObjectProperty ;
              rdfs:domain :Skill ;
              rdfs:range :SkillGroup ;
              rdfs:comment "The SkillGroup containing the Skill." ;
              rdfs:label "Parent SkillGroup" .


###  http://precis.rukmal.me/ontology#relatedTo
:relatedTo rdf:type owl:ObjectProperty ,
                    owl:SymmetricProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Accolade
                                       :Activity
                                       :Course
                                       :Degree
                                       :Portfolio
                                       :Skill
                                       :Subject
                                       :WorkExperience
                                     )
                       ] ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Accolade
                                      :Activity
                                      :Course
                                      :Degree
                                      :Portfolio
                                      :Skill
                                      :Subject
                                      :WorkExperience
                                    )
                      ] ;
           rdfs:comment "A related concept. For example, an instance of 'Award' may have a 'Project', or 'Publication' that it is related to, and vice versa." ;
           rdfs:label "Related Concept" .


###  http://precis.rukmal.me/ontology#taughtAt
:taughtAt rdf:type owl:ObjectProperty ;
          rdfs:domain :Course ;
          rdfs:range :Organization ;
          rdfs:comment "The organization at which the course was taught. This is an optional field, but ideally if this is included, the department and course codes should be too." ;
          rdfs:label "Organization which taught the course" .


#################################################################
#    Data properties
#################################################################

###  http://precis.rukmal.me/ontology#degreeConcentration
:degreeConcentration rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Degree ;
                     rdfs:range xsd:anyURI ;
                     rdfs:comment "Concentration of the Degree. For example, \"Finance and Business Economics\", or \"Computational Finance\". This should point to a DBpedia entity corresponding to the Degree being awarded." ;
                     rdfs:label "Degree Concentration" .


###  http://precis.rukmal.me/ontology#degreeGPA
:degreeGPA rdf:type owl:DatatypeProperty ;
           rdfs:domain :Degree ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:double ;
                        owl:withRestrictions ( [ xsd:minInclusive "0.0"^^xsd:double
                                               ]
                                               [ xsd:maxInclusive "4.0"^^xsd:double
                                               ]
                                             )
                      ] ;
           rdfs:comment "Grade (GPA; 4.0 scale) for the Degree (optional for obvious reasons :))." ;
           rdfs:label "Degree GPA (on a 4.0 scale)" .


###  http://precis.rukmal.me/ontology#degreeType
:degreeType rdf:type owl:DatatypeProperty ;
            rdfs:domain :Degree ;
            rdfs:range xsd:anyURI ;
            rdfs:comment "Type of the Degree. For example, \"Bachelor of Arts\", or \"Master of Science\". This should point to a DBpedia entity corresponding to the Degree being awarded." ;
            rdfs:label "Degree Type" .


###  http://precis.rukmal.me/ontology#endDate
:endDate rdf:type owl:DatatypeProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :ActivityType
                                     :Course
                                     :Portfolio
                                     :WorkExperience
                                   )
                     ] ;
         rdfs:range xsd:dateTime ;
         rdfs:comment "End date of a Instance. When this is included, the 'hasDate' data property corresponds to the start date." ;
         rdfs:label "endDate" .


###  http://precis.rukmal.me/ontology#externalResource
:externalResource rdf:type owl:DatatypeProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Accolade
                                              :ActivityType
                                              :Course
                                              :Degree
                                              :KnowledgeArea
                                              :Organization
                                              :Portfolio
                                              :SkillGroup
                                              :WorkExperience
                                            )
                              ] ;
                  rdfs:range xsd:anyURI ;
                  rdfs:comment "Reference to an external resource that describes the same entity. This would typically be a reference to an existing DBpedia concept that describes the same entity. DBpedia references are not directly used here to mitigate the fact that certain user-created entitites will not have existing concepts on DBpedia, and so that we can ensure the existence of some other descriptive fields." ;
                  rdfs:label "External resource" .


###  http://precis.rukmal.me/ontology#hasAuthors
:hasAuthors rdf:type owl:DatatypeProperty ;
            rdfs:domain :Publication ;
            rdfs:range xsd:string ;
            rdfs:comment "List of authors (Human readable for now). This list should mirror the order of authors in the publication. Note that eventually this will be replaced by pointers to individual \"Author\" Classes." ;
            rdfs:label "Author List" .


###  http://precis.rukmal.me/ontology#hasCollaborators
:hasCollaborators rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Portfolio ;
                  rdfs:range xsd:string ;
                  rdfs:comment "List of Collaborators (Human-readable for now). Will eventually change to a mapping to a Person entity." ;
                  rdfs:label "Collaborators List" .


###  http://precis.rukmal.me/ontology#hasCourseCode
:hasCourseCode rdf:type owl:DatatypeProperty ;
               rdfs:domain :Course ;
               rdfs:range xsd:string ;
               rdfs:comment "Code of the course (this would imply that an organization is referenced too). For example: 420, 690, 069, etc." ;
               rdfs:label "Course code" .


###  http://precis.rukmal.me/ontology#hasDOI
:hasDOI rdf:type owl:DatatypeProperty ;
        rdfs:domain :Publication ;
        rdfs:range xsd:anyURI ;
        rdfs:comment "Digital Object Identifier (DOI) of the publication. Including this will allow for later expansion if an inference agent is ever added to get more metadata using this." ;
        rdfs:label "Digital Object Identifier (DOI) of the Publication" .


###  http://precis.rukmal.me/ontology#hasDate
:hasDate rdf:type owl:DatatypeProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Accolade
                                     :ActivityType
                                     :Course
                                     :Degree
                                     :Portfolio
                                     :WorkExperience
                                   )
                     ] ;
         rdfs:range xsd:dateTime ;
         rdfs:comment "Date of the instance. This implies \"startDate\" when a range is given with \"endDate\"." ;
         rdfs:label "Date" .


###  http://precis.rukmal.me/ontology#hasDepartmentCode
:hasDepartmentCode rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Course ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Code of the department (this would imply that an organization is referenced too). For example: FIN, CFRM, FE, etc." ;
                   rdfs:label "Department code" .


###  http://precis.rukmal.me/ontology#hasImage
:hasImage rdf:type owl:DatatypeProperty ;
          rdfs:domain :WorkExperience ;
          rdfs:range xsd:anyURI ;
          rdfs:comment "URL to image related to work experience. Can be used when displaying the information at consumption." ;
          rdfs:label "Image" .


###  http://precis.rukmal.me/ontology#hasMedia
:hasMedia rdf:type owl:DatatypeProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Accolade
                                      :ActivityType
                                      :Portfolio
                                      :WorkExperience
                                    )
                      ] ;
          rdfs:range xsd:anyURI ;
          rdfs:comment "Additional media can be included here. For example, it could link to product/team pages, news articles, etc." ;
          rdfs:label "Media" .


###  http://precis.rukmal.me/ontology#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Accolade
                                     :ActivityType
                                     :Course
                                     :Degree
                                     :KnowledgeArea
                                     :Organization
                                     :Portfolio
                                     :SkillGroup
                                     :WorkExperience
                                   )
                     ] ;
         rdfs:range xsd:string ;
         rdfs:comment "Human readable name for the entity; for display. This is the primary descriptive string for the instance it is mapped to; that is, for 'WorkExperience' it would be the job title, and for 'Publication' it would be the publication title." ;
         rdfs:label "Human readable name" .


###  http://precis.rukmal.me/ontology#hasPriority
:hasPriority rdf:type owl:DatatypeProperty ;
             rdfs:domain :Description ;
             rdfs:range xsd:nonNegativeInteger ;
             rdfs:comment "Priority level of the description (used for deciding truncation in toolkit). This is automatically set to the position of the string in the input \"description\" array of descriptions, when data is loaded using the command line tool." ;
             rdfs:label "description priority level" .


###  http://precis.rukmal.me/ontology#hasStatus
:hasStatus rdf:type owl:DatatypeProperty ;
           rdfs:domain :Publication ;
           rdfs:range xsd:string ;
           rdfs:comment "Status of the publication. That is, this property should reflect a paper being \"In Progress\", or \"Under Review\"." ;
           rdfs:label "Publication Status" .


###  http://precis.rukmal.me/ontology#hasText
:hasText rdf:type owl:DatatypeProperty ;
         rdfs:domain :Description ;
         rdfs:range xsd:string ;
         rdfs:comment "Text of the description. This should be treated as a single \"bullet point\" on a traditional Resume/CV." ;
         rdfs:label "description text" .


###  http://precis.rukmal.me/ontology#hasWebsite
:hasWebsite rdf:type owl:DatatypeProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Accolade
                                        :ActivityType
                                        :Course
                                        :Degree
                                        :Organization
                                        :Portfolio
                                        :SkillGroup
                                        :WorkExperience
                                      )
                        ] ;
            rdfs:range xsd:anyURI ;
            rdfs:comment "External website of the entity (not used for ontology extension; treat this as user-facing)." ;
            rdfs:label "Entity website" .


###  http://precis.rukmal.me/ontology#inCity
:inCity rdf:type owl:DatatypeProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Accolade
                                    :ActivityType
                                    :Course
                                    :Degree
                                    :Organization
                                    :Portfolio
                                    :SkillGroup
                                    :WorkExperience
                                  )
                    ] ;
        rdfs:range xsd:string ;
        rdfs:comment "The city of the entity." ;
        rdfs:label "City" .


###  http://precis.rukmal.me/ontology#inConferenceOrJournal
:inConferenceOrJournal rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Publication ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Conference/Journal name. This will eventually be replaced with a more semantically-correct representation of this information." ;
                       rdfs:label "Conference/Journal Name" .


###  http://precis.rukmal.me/ontology#inCountry
:inCountry rdf:type owl:DatatypeProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Accolade
                                       :ActivityType
                                       :Course
                                       :Organization
                                       :Portfolio
                                       :SkillGroup
                                       :WorkExperience
                                     )
                       ] ;
           rdfs:range xsd:string ;
           rdfs:comment "Country of the entity." ;
           rdfs:label "Country" .


###  http://precis.rukmal.me/ontology#inPublication
:inPublication rdf:type owl:DatatypeProperty ;
               rdfs:domain :Publication ;
               rdfs:range xsd:string ;
               rdfs:comment "Publication name containing the specific publication. As with the other fields in the 'Publication' concept, this will probably be replaced eventually." ;
               rdfs:label "Publication Name" .


###  http://precis.rukmal.me/ontology#inState
:inState rdf:type owl:DatatypeProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Accolade
                                     :ActivityType
                                     :Course
                                     :Degree
                                     :Organization
                                     :Portfolio
                                     :SkillGroup
                                     :WorkExperience
                                   )
                     ] ;
         rdfs:range xsd:string ;
         rdfs:comment "State of the entity (United States only)." ;
         rdfs:label "State" .


###  http://precis.rukmal.me/ontology#otherTitles
:otherTitles rdf:type owl:DatatypeProperty ;
             rdfs:domain :WorkExperience ;
             rdfs:range xsd:string ;
             rdfs:comment "'Other' job titles; this property should be specified in addition to the main 'hasName' property, and is considered peripheral information." ;
             rdfs:label "Other Titles" .


###  http://precis.rukmal.me/ontology#withGrade
:withGrade rdf:type owl:DatatypeProperty ;
           rdfs:domain :Course ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:double ;
                        owl:withRestrictions ( [ xsd:minInclusive "0.0"^^xsd:double
                                               ]
                                               [ xsd:maxInclusive "4.0"^^xsd:double
                                               ]
                                             )
                      ] ;
           rdfs:comment "Grade (GPA; 4.0 scale) received in the course (optional for obvious reasons :))." ;
           rdfs:label "Course GPA (on a 4.0 scale)" .


#################################################################
#    Classes
#################################################################

###  http://precis.rukmal.me/ontology#Accolade
:Accolade rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :affiliatedWith ;
                            owl:someValuesFrom [ rdf:type owl:Class ;
                                                 owl:unionOf ( :Degree
                                                               :WorkExperience
                                                             )
                                               ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDescription ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Description
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :externalResource ;
                            owl:someValuesFrom xsd:anyURI
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasMedia ;
                            owl:someValuesFrom xsd:anyURI
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDate ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasName ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ;
          rdfs:comment "An accolade. This is either an 'Award' or a 'Certification'." ;
          rdfs:label "Accolade" .


###  http://precis.rukmal.me/ontology#Activity
:Activity rdf:type owl:Class ;
          rdfs:subClassOf :ActivityType ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :inActivityType ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :ActivityType
                          ] ;
          rdfs:comment "A specific activity, such as a specific hackathon or volunteering experience." ;
          rdfs:label "Activity" .


###  http://precis.rukmal.me/ontology#ActivityType
:ActivityType rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :hasActivity ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
